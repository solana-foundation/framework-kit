{
	"name": "framework-kit",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@10.20.0",
	"scripts": {
		"dev": "turbo run dev --parallel",
		"dev:react": "pnpm --filter @solana/example-vite-react dev",
		"build": "turbo run build",
		"format": "turbo run format",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"typecheck": "turbo run typecheck",
		"size": "pnpm build && size-limit",
		"prepare": "husky",
		"changeset": "changeset",
		"version:packages": "changeset version",
		"publish-packages": "changeset publish --filter @solana/client --filter @solana/react-hooks --filter @solana/web3-compat"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.7",
		"@size-limit/preset-small-lib": "^12.0.0",
		"@solana/codecs-strings": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@vitest/coverage-v8": "^4.0.7",
		"bs58": "^6.0.0",
		"husky": "^9.1.7",
		"jsdom": "^24.1.0",
		"size-limit": "^12.0.0",
		"tsup": "^8.5.0",
		"turbo": "^2.6.0",
		"typescript": "^5.6.3",
		"vitest": "^4.0.7"
	}
}
